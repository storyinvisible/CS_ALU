module boolean (
    input alufn[4],
    input a[16],
    input b[16],
    output out[16]
    
  ) {
  var i;
  always {
    out=16b0;
    for (i=0; i<16; i++){
      case(alufn[3:0]){
        4b1000:
          out[i]= a[i]& b[i];
        4b1110:
          out[i] = a[i]|b[i];
        4b0110:
          out[i] = a[i]^b[i];
        4b1010:
          out[i]= a[i];
        
        }
      }
    
  }
}
